    skos:definition "The year of birth for the patient."@en;
    rdfs:seeAlso <https://care-sm.readthedocs.io/en/latest/data_elements.html#birthyear> .

<https://w3id.org/CARE-SM#birthplace>
    a skos:Concept ;
